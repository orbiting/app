{
  "name": "orbitingapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "run-android": "node ./scripts/run-android.js",
    "run-ios": "node ./scripts/run-ios.js",
    "run-android-dev": "react-native run-android --variant devDebug",
    "run-android-staging": "react-native run-android --variant stagingDebug",
    "run-android-production": "react-native run-android --variant productionDebug",
    "run-ios-dev": "react-native run-ios --scheme 'orbitingapp (dev)'",
    "run-ios-staging": "react-native run-ios --scheme 'orbitingapp (staging)'",
    "run-ios-production": "react-native run-ios --scheme 'orbitingapp (production)'",
    "deploy-android-alpha": "cd ./android && fastlane alpha && cd -",
    "deploy-ios-beta": "cd ./ios && fastlane ios beta && cd -",
    "translations": "gsheets --key=1KFxXKjFplXM97N9ssk55oJol0FttDfwYiR9MJSQwpqw --title=live --pretty --out src/utils/translations.json"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@project-r/styleguide": "^5.78.1",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.3.1",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "apollo-link-ws": "^1.0.8",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "lodash.debounce": "^4.0.8",
    "react": "16.3.1",
    "react-apollo": "^2.1.4",
    "react-json-tree": "^0.11.0",
    "react-native": "0.55.4",
    "react-native-code-push": "^5.3.3",
    "react-native-config": "^0.11.5",
    "react-native-cookies": "^3.3.0",
    "react-native-push-notification": "^3.0.2",
    "react-native-spinkit": "^1.1.1",
    "react-native-splash-screen": "^3.0.6",
    "react-native-wkwebview-reborn": "https://github.com/orbiting/react-native-wkwebview",
    "react-navigation": "^2.0.1",
    "recompose": "^0.27.0",
    "subscriptions-transport-ws": "^0.9.10",
    "url-parse": "^1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "gsheets": "^2.0.0",
    "husky": "^0.14.3",
    "inquirer": "^5.2.0",
    "lint-staged": "^6.1.0",
    "standard": "^10.0.3"
  }
}
