{
  "name": "orbitingapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "deploy-ios": "fastlane ios release",
    "deploy-ios-beta": "cd ./ios && fastlane beta && cd -",
    "deploy-android-alpha": "cd ./android && fastlane alpha && cd -",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/build/Build/Products/Release-iphoneos/orbitingapp.app/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios/build/Build/Products/Release-iphoneos/orbitingapp.app'"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-boost": "^0.1.6",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-cache-persist": "^0.1.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "lodash.debounce": "^4.0.8",
    "react": "16.3.1",
    "react-apollo": "^2.1.4",
    "react-json-tree": "^0.11.0",
    "react-native": "0.55.4",
    "react-native-code-push": "^5.3.3",
    "react-native-config": "^0.11.5",
    "react-native-cookies": "^3.3.0",
    "react-native-push-notification": "^3.0.2",
    "react-native-spinkit": "^1.1.1",
    "react-native-splash-screen": "^3.0.6",
    "react-native-wkwebview-reborn": "^1.20.0",
    "react-navigation": "^2.0.1",
    "recompose": "^0.27.0",
    "url-parse": "^1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "standard": "^10.0.3"
  }
}
